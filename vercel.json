{
    "version":2,
    "builds":[
        {
             "src": "poll/wsgi.py",
             "use": "@vercel/python",
             "confi": { "maxLambdaSize": "15mb", "runtime":"python3.11.4"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "disDir": "staticsfiles_build"
            }
        }
       
    ],
    "routes":[{
          "src": "/static/(.*)",
          "dest": "/static/$1"
    },
{
    "src": "/(.*)",
    "dest": "poll/wsgi.py"  
}

]
}